{
  "name": "web-desk",
  "version": "0.8.1",
  "description": "Launch any webpage as desktop application.",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Sanjeev Premi",
    "email": "spremi@ymail.com",
    "url": "https://github.com/spremi"
  },
  "main": "dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ele-build": "cd electron && tsc",
    "ele-watch": "cd electron && tsc -w",
    "ele-lint": "cd electron && eslint -c .eslintrc --ext .ts ./src",
    "ele-start": "electron dist/main.js",
    "ele-seed": "cp starter.json dist/config.json",
    "pkg-linux-64": "electron-packager . web-desk --overwrite --asar --platform=linux --arch=x64  --icon=src/assets/app-logo.png --prune=true --out=pkg",
    "rpm-linux-64": "electron-installer-redhat --config rpm64.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.15",
    "@angular/cli": "^11.2.15",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "codelyzer": "^6.0.0",
    "electron": "^15.2.0",
    "electron-installer-redhat": "^3.3.0",
    "electron-packager": "^15.3.0",
    "eslint": "^7.12.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
