{
  "name": "web-desk",
  "version": "0.8.6",
  "description": "Launch any webpage as desktop application.",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Sanjeev Premi",
    "email": "spremi@ymail.com",
    "url": "https://github.com/spremi"
  },
  "main": "dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ele-build": "cd electron && tsc",
    "ele-watch": "cd electron && tsc -w",
    "ele-lint": "cd electron && eslint -c .eslintrc --ext .ts ./src",
    "ele-start": "electron dist/main.js",
    "ele-seed": "cp starter.json dist/config.json",
    "pkg-linux-64": "electron-packager . web-desk --overwrite --asar --platform=linux --arch=x64  --icon=src/assets/app-logo.png --prune=true --out=pkg",
    "deb-linux-64": "electron-installer-debian --config deb64.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~12.2.15",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.15",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "^12.2.15",
    "@angular/compiler-cli": "~12.2.15",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "electron": "^15.3.5",
    "electron-installer-debian": "^3.1.0",
    "electron-packager": "^15.4.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  }
